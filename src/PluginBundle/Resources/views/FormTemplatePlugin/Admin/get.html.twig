<h4>{{ 'plugin.form_template.title'|trans }}</h4>
<dl class="dl-horizontal">
   <dt>{{ 'plugin.form_template.conf.formType'|trans }}</dt><dd>{{ pluginData.formType }}</dd>
   <dt>{{ 'plugin.form_template.conf.admin_enrollment_list_fields'|trans }}</dt><dd>
      {% for field in pluginData.admin_enrollment_list_fields %}
         {{ field }}<br>
      {% endfor %}
   </dd>
   {% if pluginData.config is defined %}
      <dt>{{ 'plugin.form_template.conf.config'|trans }}</dt><dd><pre>{{ pluginData.config|yaml_encode(1) }}</pre></dd>
   {% endif %}
</dl>
