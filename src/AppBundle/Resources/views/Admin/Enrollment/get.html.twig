{% extends 'AppBundle:Admin/Enrollment:skeleton.html.twig' %}
{% block content %}
    <div class="panel-heading">
        <h3 class="panel-title">{{ icon('file-text-o') }} Enrollment for {{ form.name }}</h3>
    </div>
    <div class="panel-body">
        {% for template in data.templates %}
            {% include template with data.templates[template] %}
        {% endfor %}
    </div>
{% endblock %}
{% block javascript %}
    {{ parent() }}
    <script>
        $('[data-onload=onchange][onchange]').trigger('onchange');
    </script>
{% endblock %}
