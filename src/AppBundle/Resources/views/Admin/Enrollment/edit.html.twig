{% extends 'AppBundle:Admin/Enrollment:skeleton.html.twig' %}
{% block title %}{% trans %}Edit{% endtrans %} | {{ parent() }}{% endblock %}
{% block content %}
    <div class="panel-heading">
        <h3 class="panel-title">{{ icon('file-text-o') }} {% trans %}Edit enrollment for{% endtrans %} {{ form.name }}</h3>
    </div>
    <div class="panel-body">
        {% for template in userFormEvent.templates %}
            {% include template with userFormEvent.templates[template] %}
        {% endfor %}
    </div>
    <div class="panel-footer">
        {{ form(adminForm) }}
    </div>
{% endblock %}
{% block javascript %}
    {{ parent() }}
    <script>
        $('[data-onload=onchange][onchange]').trigger('onchange');
    </script>
{% endblock %}
