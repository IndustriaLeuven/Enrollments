{% extends 'AppBundle:Admin/Form:skeleton.html.twig' %}
{% block title %}{% trans %}Edit{% endtrans %} {{ form.name }} | {{ parent() }}{% endblock %}
{% block content %}
    <div class="panel-heading">
        <h3 class="panel-title">{{ icon('list-alt') }} {% trans %}Edit{% endtrans %} {{ form.name }}</h3>
    </div>
    <div class="panel-body">
        {{ form(data) }}
    </div>
{% endblock %}
{% block javascript %}
    {{ parent() }}
    <script src="{{ asset('bundles/app/js/form_plugins.js') }}"></script>
{% endblock %}
