{% extends 'AppBundle:Admin/Form:skeleton.html.twig' %}
{% block title %}Create form | {{ parent() }}{% endblock %}
{% block content %}
    <div class="panel-heading">
        <h3 class="panel-title">{{ icon('list-alt') }} Create form</h3>
    </div>
    <div class="panel-body">
        {% form_theme data "AppBundle:Form:form_label_doc.html.twig" %}
        {{ form(data) }}
    </div>
{% endblock %}
{% block javascript %}
    {{ parent() }}
    <script src="{{ asset('bundles/app/js/form_plugins.js') }}"></script>
{% endblock %}
